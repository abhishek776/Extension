{
    "name": "NativeDoc",
    "version": "0.9",
    "manifest_version" : 2,
    "background" : {
	"scripts" : ["event.js", "jquery.js"],
	"persistent" : true
    },
    "description": "Viewer for docx and pptx",
    "permissions": ["webRequest",
            "webRequestBlocking",
            "webNavigation",
		    "tabs",
		    "*://*/*"],
